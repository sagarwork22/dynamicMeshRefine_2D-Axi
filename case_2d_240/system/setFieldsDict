FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
       cylinderToCell
    {
        p1 ( 0 0 -0.001 );
        p2 ( 0 0  0.001 );
        radius   0.001;
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
 
 );


